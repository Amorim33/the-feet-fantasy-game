{
  "name": "the-feet-fantasy",
  "version": "0.0.1",
  "keywords": [
    "phaser",
    "typescript",
    "game",
    "html5",
    "webgl"
  ],
  "license": "GPL-3.0",
  "type": "module",
  "main": "src/game.ts",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.js",
    "dev": "webpack --mode development && webpack serve --mode development",
    "format:check": "prettier --cache --check .",
    "format:fix": "prettier --cache --write .",
    "lint": "eslint --report-unused-disable-directives --max-warnings=0 \"**/*.{ts*,js,cjs}\"",
    "lint:fix": "eslint --report-unused-disable-directives --max-warnings=0 \"**/*.{ts*,js,cjs}\" --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "phaser": "^3.70.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.4",
    "prettier": "^3.1.1",
    "prettier-plugin-jsdoc": "^1.1.1",
    "prettier-plugin-packagejson": "^2.4.6",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
